{
  "manifest_version": 3,
  "name": "4_FolderUpload",
  "version": "1.0",
  "icons": {
  "16": "icons/icon16.png",
  "32": "icons/icon32.png",
  "48": "icons/icon48.png",
  "128": "icons/icon128.png",
  "256": "icons/icon256.png"
  },
  "description": "Uploads a selected folder to Google Drive which folder ID comes from gogolesheets",
  "permissions": [
    "identity"
  ],
  "host_permissions": [
    "https://www.googleapis.com/"
  ],
  "action": {
    "default_title": "Folder Uploader",
    "default_popup": "popup.html"
  },
  "oauth2": {
   "client_id": "490906539985-6js9v06ndtnvc01tkuqkp3opk8gig1ea.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.file"
    ]
  },
  "web_accessible_resources": [
    {
      "resources": [
        "uploader.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
